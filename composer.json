{
    "name": "leeroy/cms",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Marc Giroux",
            "email": "marc@leeroy.ca"
        },
        {
            "name": "Philippe Bédard De Launière",
            "email": "philippe@leeroy.ca"
        }
    ],
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "require": {
        "php": ">=8.1",
        "ext-mongodb": "*",
        "leeroy/sailcms": "dev-feature/cms-entry"
    },
    "autoload": {
        "psr-4": {
            "Page\\": "containers/page"
        }
    },
    "sailcms": {
        "containers": [
            "Page"
        ],
        "modules": []
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "/home/delau/projects/sailcms"
        }
    ]
}
