{
    "name": "leeroy/sailcms-app",
    "license": "MIT",
    "description": "SailCMS installer project for composer",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Marc Giroux",
            "email": "marc@leeroy.ca"
        }
    ],
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "pestphp/pest": "^1.22"
    },
    "require": {
        "php": ">=8.1",
        "leeroy/sailcms": "dev-release/3.0.0"
    },
    "autoload": {
        "psr-4": {
        }
    },
    "sailcms": {
        "containers": [],
        "modules": []
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest"
    }
}
